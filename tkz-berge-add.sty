  \pgfmathsetmacro{\the@yc}{\y@coord+\the@yheight*sin(90+\the@rot)}
  
  \Vertex[x=\the@xc,y=\the@yc]{\the@prefix4}

  \Edge(\the@prefix0)(\the@prefix4)
  \Edge(\the@prefix1)(\the@prefix4)

  %\ifcase\cmdGR@cl@form
