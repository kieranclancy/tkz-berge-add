  \edef\y@coord{\cmdGR@cl@y}
  \edef\r@coord{\cmdGR@cl@r}
  \edef\d@coord{\cmdGR@cl@d}
  \edef\the@prefix{\cmdGR@cl@prefix}
  \edef\the@rad{\cmdGR@cl@RA}
  \edef\the@rot{\cmdGR@cl@rotation}

  \grCycle[%
  prefix=\the@prefix,
  x=\x@coord,
